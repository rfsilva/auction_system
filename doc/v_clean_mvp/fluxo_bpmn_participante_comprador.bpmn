<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_Comprador"
                  targetNamespace="http://example.com/bpmn">

  <!-- Pool -->
  <bpmn:collaboration id="Collaboration_Comprador">
    <bpmn:participant id="Participant_Comprador" name="Comprador" processRef="Process_Comprador"/>
  </bpmn:collaboration>

  <!-- Processo do Comprador -->
  <bpmn:process id="Process_Comprador" isExecutable="false" name="Fluxo do Comprador">

    <!-- Evento Inicial -->
    <bpmn:startEvent id="StartEvent_Comprador" name="Acessa plataforma">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <!-- Tarefas -->
    <bpmn:task id="Task_Login" name="Login">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_NavegarProdutos" name="Navegar e buscar produtos">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_SelecionarProduto" name="Selecionar produto para participar do leilão">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_DarLance" name="Dar lance">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_AcompanharLeilao" name="Acompanhar leilão em tempo real">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_HistoricoFavoritos" name="Consultar histórico e favoritos">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:task>

    <!-- Evento Final -->
    <bpmn:endEvent id="EndEvent_Comprador" name="Saída">
      <bpmn:incoming>Flow_7</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Sequência de fluxos -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_Comprador" targetRef="Task_Login"/>
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_Login" targetRef="Task_NavegarProdutos"/>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_NavegarProdutos" targetRef="Task_SelecionarProduto"/>
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_SelecionarProduto" targetRef="Task_DarLance"/>
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_DarLance" targetRef="Task_AcompanharLeilao"/>
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_AcompanharLeilao" targetRef="Task_HistoricoFavoritos"/>
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Task_HistoricoFavoritos" targetRef="EndEvent_Comprador"/>

  </bpmn:process>

  <!-- Diagrama visual -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_Comprador">
    <bpmndi:BPMNPlane id="BPMNPlane_Comprador" bpmnElement="Process_Comprador">

      <!-- Evento Inicial -->
      <bpmndi:BPMNShape id="Shape_StartEvent" bpmnElement="StartEvent_Comprador">
        <dc:Bounds x="100" y="150" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <!-- Tarefas -->
      <bpmndi:BPMNShape id="Shape_Login" bpmnElement="Task_Login">
        <dc:Bounds x="160" y="140" width="100" height="50"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Shape_NavegarProdutos" bpmnElement="Task_NavegarProdutos">
        <dc:Bounds x="280" y="140" width="120" height="50"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Shape_SelecionarProduto" bpmnElement="Task_SelecionarProduto">
        <dc:Bounds x="420" y="140" width="140" height="50"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Shape_DarLance" bpmnElement="Task_DarLance">
        <dc:Bounds x="580" y="140" width="100" height="50"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Shape_AcompanharLeilao" bpmnElement="Task_AcompanharLeilao">
        <dc:Bounds x="700" y="140" width="160" height="50"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Shape_HistoricoFavoritos" bpmnElement="Task_HistoricoFavoritos">
        <dc:Bounds x="880" y="140" width="140" height="50"/>
      </bpmndi:BPMNShape>

      <!-- Evento Final -->
      <bpmndi:BPMNShape id="Shape_EndEvent" bpmnElement="EndEvent_Comprador">
        <dc:Bounds x="1050" y="150" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <!-- Pool -->
      <bpmndi:BPMNShape id="Shape_Pool_Comprador" bpmnElement="Participant_Comprador" isHorizontal="true">
        <dc:Bounds x="50" y="100" width="1060" height="120"/>
      </bpmndi:BPMNShape>

      <!-- Fluxos -->
      <bpmndi:BPMNEdge id="Edge_Flow_1" bpmnElement="Flow_1">
        <di:waypoint x="136" y="168"/>
        <di:waypoint x="160" y="165"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Edge_Flow_2" bpmnElement="Flow_2">
        <di:waypoint x="260" y="165"/>
        <di:waypoint x="280" y="165"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Edge_Flow_3" bpmnElement="Flow_3">
        <di:waypoint x="400" y="165"/>
        <di:waypoint x="420" y="165"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Edge_Flow_4" bpmnElement="Flow_4">
        <di:waypoint x="560" y="165"/>
        <di:waypoint x="580" y="165"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Edge_Flow_5" bpmnElement="Flow_5">
        <di:waypoint x="680" y="165"/>
        <di:waypoint x="700" y="165"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Edge_Flow_6" bpmnElement="Flow_6">
        <di:waypoint x="860" y="165"/>
        <di:waypoint x="880" y="165"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Edge_Flow_7" bpmnElement="Flow_7">
        <di:waypoint x="1020" y="165"/>
        <di:waypoint x="1050" y="168"/>
      </bpmndi:BPMNEdge>

    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>
